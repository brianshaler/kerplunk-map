{
  "name": "kerplunk-map",
  "displayName": "Map Component",
  "description": "Experimental embed of self-hosted mapbox-gl tile server and react component.",
  "version": "1.0.0",
  "kerplunk": {
    "services": {
      "tileserver": {
        "private": true,
        "image": "brianshaler/restart-tileserver-gl:0.8",
        "volumes": [
          "/data"
        ],
        "ports": [
          "80/tcp"
        ]
      }
    }
  },
  "keywords": [
    "kerplunk-plugin",
    "kp:Location",
    "kp:Map",
    "kp:Visualization"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "leaflet-hash": "^0.2.1",
    "lodash": "^4.6.1",
    "mapbox": "^0.12.0",
    "mapbox-gl": "^0.21.0",
    "react": "^0.13.3",
    "request": "^2.73.0",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "glut": "0.0.1",
    "glut-coffee-amdify": "0.0.0",
    "gulp": "^3.8.8",
    "gulp-coffee": "^2.3.1",
    "mocha": "^2.3.2",
    "should": "^7.1.0"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib ./public && gulp",
    "test": "NODE_ENV=test mocha"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brianshaler/kerplunk-map.git"
  },
  "license": "MIT"
}
